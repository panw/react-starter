{
  "name": "react-babel-webpack-express-starter",
  "version": "0.0.1",
  "description": "Basic starter code for React, Webpack, Babel, and Express",
  "main": "server.js",
  "scripts": {
    "build:dev": "webpack-dev-server --config webpack-dev-server.config.js --inline --hot --progress --colors",
